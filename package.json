{
  "name": "e2e-client-cypress-template",
  "version": "1.0.0",
  "description": "Repository Template for Cypress Projects",
  "main": "index.js",
  "scripts": {
    "run:chrome": "npx cypress run --browser chrome",
    "run:edge": "npx cypress run --browser edge",
    "open:chrome": "npx cypress run --browser chrome",
    "open:edge": "npx cypress run --browser edge"
  },
  "eslintConfig": {
    "plugins": [
      "cypress"
    ],
    "rules": {
      "cypress/no-assigning-return-values": "error",
      "cypress/no-unnecessary-waiting": "error",
      "cypress/assertion-before-screenshot": "warn",
      "cypress/no-async-tests": "error",
      "cypress/no-pause": "error",
      "cypress/unsafe-to-chain-command": "warn"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "cypress": "^13.1.0",
    "cypress-plugin-steps": "^1.1.1",
    "cypress-wait-until": "^2.0.1",
    "cypress-xpath": "^2.0.1",
    "dotenv": "^16.3.1",
    "eslint-plugin-cypress": "^2.14.0"
  }
}
